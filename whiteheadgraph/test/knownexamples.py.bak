import word
import whiteheadgraph.build.wgraph as wg
# a few words
com=word.word([1,2,-1,-2])
a=word.word([1])
b=word.word('b')
bs12=word.word([1,2,-1,-1,-2])
bs13=word.word('abAAAB')
tk=[word.word('abAB'),word.word('abaB')]
bs12a=[bs12,a]
k6=[word.word('b'),word.word('ca'),word.word('cAB'),word.word('abC')]
baumslag=word.word('AABAbaBab')
k33=word.word('bbaaccabc')


# their corresponding Whitehead graphs
Com=wg.WGraph([com])
#A=wg.WGraph(a)
#B=wg.WGraph(b)
BS12=wg.WGraph([bs12])
BS13=wg.WGraph([bs13])
TK=wg.WGraph(tk)
K4=wg.WGraph([a,b,com]) # rigid
K4HNN=wg.WGraph([a,word.word('aBabABAb')])
BS12A=wg.WGraph([bs12,a])
K6=wg.WGraph(k6) # rigid
AS=wg.WGraph([a,b,word.word('abaBAbAB')]) # rigid 'almost splits'
NonTree=wg.WGraph([word.word('Abc'),word.word('Acb'),word.word('Abbb'),word.word('Accc')]) # rigid but rigid cube complex is not a tree
K33=wg.WGraph([k33])
Baumslag=wg.WGraph([baumslag])

